	<!-- Mega menu 1 Start-->
	<template id="megamenu_1" name="Mega Menu 1">
        <section class="container-fluid py-3 px-3 megamenu_1">
			  <div class="">
            <div class="row">
				<t t-set="menu1_title">First Menu</t>
                <t t-set="menu2_title">Second Menu</t>
                <t t-set="menu3_title">Third Menu</t>
                <t t-set="menu4_title">fourth Menu</t>
                <t t-set="menu5_title">Fifth Menu</t>
                <t t-set="menu6_title">Last Menu</t>
                <t t-foreach="[menu1_title, menu2_title, menu3_title, menu4_title,menu5_title,menu6_title]" t-as="menu_title">
                    <div class="col-lg-2 col-md-4 col-sm-6">
                    <div class="img_container">
                        <div class="">
                            <img class="img img-responsive img-fluid d-block mx-auto" alt="Mega Menu Style 1"
                                 src="/website_megamenus/static/src/img/350x250.jpg"></img>
                        </div>
                    </div>
                    <section class="py-lg-3 py-md-3 py-sm-3 py-2 text-center text-sm-left">
                         <nav class="nav flex-column">
                            <t t-foreach="4" t-as="i">
                                <t t-set="text">Menu Item %s</t>
                                <t t-set="text" t-value="text % (i + 1)"/>
                                <a href="#" class="nav-link" data-name="Menu Item"
                                   t-esc="text"/>
                            </t>
                        </nav>
                    </section>
                </div>
                </t>
            </div>
            </div>
        </section>
    </template>
    <!-- Mega menu 1 End -->
