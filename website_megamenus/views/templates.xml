<odoo>
	 <!--=============== Assets List================== -->
	 <template id="assets_frontend_mega_menu" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
			<link rel="stylesheet" type="text/css" href="/website_megamenus/static/src/scss/style.scss" />
		</xpath>

		<xpath expr="script[last()]" position="after">
			 <script type="text/javascript" src="/website_megamenus/static/src/js/menu.js"/>
		</xpath>

	</template>			
    <!--=============== Mega Menu List================== -->
    <template id="megamenu_snippets" inherit_id="website.snippets">
		 <xpath expr="//div[@id='snippet_mega_menu']/div[hasclass('o_panel_body')]" position="inside">
			  <t t-snippet="website_megamenus.megamenu_1"
               t-thumbnail="/website_megamenus/static/src/img/menu1.png"/>
				<t t-snippet="website_megamenus.megamenu_2"
